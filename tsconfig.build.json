{
  "compilerOptions": {
    "types": ["vite/client"],
    "composite": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "lib": ["esnext", "dom"],
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules"]
}
